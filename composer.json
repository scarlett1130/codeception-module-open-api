{
    "name": "meetmatt/openapi-spec-coverage",
    "description": "Codeception Module - Extended REST Module with API spec coverage",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Iurii Golikov",
            "email": "iurii@golikov.lu"
        }
    ],
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/meetmatt/module-rest.git"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "slim/slim": "^4.9",
        "slim/psr7": "^1.5"
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.3",
        "codeception/module-rest": "dev-coverage",
        "docler-labs/codeception-slim-module": "^2.0"
    },
    "config": {
        "platform": {
            "php": "7.4.99"
        },
        "sort-packages": true
    },
    "autoload-dev": {
        "psr-4": {
            "MeetMatt\\RestCoverage\\Test\\Suite\\Functional\\": "test/suite/functional",
            "MeetMatt\\RestCoverage\\Test\\Support\\": "test/support"
        }
    }
}
